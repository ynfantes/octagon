<script src="{{constant('ROOT')}}js/lib/venobox.min.js"></script>
<script src="{{constant('ROOT')}}js/custom.js"></script>
<script>
    $("#login-form").validate({
        debug:true,
        rules:{
            nombre:{required:true,minlength:2},
            password:{required:true,minlength:4,maxlength:20}
        },
        messages:{
            nombre:{required:'Usuario es requerido',minlength:'Ingrese mímino 4 caracteres'},
            password:{
                required:'Ingrese su contraseña',
                minlength:'Deben ser mínimo 4 caracteres',
                maxlength:'No pueden ser más de 20 caracteres'}
        },
        submitHandler:function(form){
            form.submit();
            $("btn-submit").attr("disabled","disabled");
        },
        errorPlacement:function(error,element){
            error.insertBefore(element.parent());
        }
    });
</script>
